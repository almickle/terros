{
  "name": "@terros/utils",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --tsconfig tsconfig.build.json",
    "clean": "rimraf dist",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch --tsconfig tsconfig.build.json",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "test": "vitest"
  },
  "dependencies": {
    "nanoid": "^5.0.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.8.0",
    "rimraf": "^6.0.1",
    "tsup": "^8.0.2",
    "type-fest": "^4.0.0",
    "typescript": "^5.8.3",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.0.0"
  }
}
